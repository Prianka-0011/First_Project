@model List<NikuHotel.Models.ApplicationUser>
@{
    ViewData["Title"] = "RegisteredUserIndex";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}



<div class=" row">
    <div class="col-md-10">
        
    </div>
</div>
<div class="row">

    <div class="col-md-12">
        <h2 class="text-center text-primary">RegisteredUserIndex</h2><br><br>
        <div class="table-bordered bg-fixed bg-dark">
            <br><br>
            <div class="col-md-8">
                <table class="table table-bordered table-responsive table-hover">
                    <thead class="bg-danger">
                        <tr class="text-white">
                            <td>Email</td>
                            <td>Pasword</td>
                        </tr>

                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr class="text-white">
                                <td>@item.Email</td>
                                <td>@item.PasswordHash</td>

                            </tr>

                        }

                    </tbody>
                </table><br><br>
            </div>
        </div>
    </div>
</div>